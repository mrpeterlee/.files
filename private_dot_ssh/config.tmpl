# SSH Config - managed by chezmoi

Host *
{{- if not .isWindows }}
    ForwardX11 yes
{{- end }}
    ServerAliveInterval 60
    ServerAliveCountMax 60

Host acap
    HostName 52.221.6.72
    User peter
    IdentityFile ~/.ssh/acap-admin-key.pem

# Host tapai-admin
    # HostName 13.251.46.169
